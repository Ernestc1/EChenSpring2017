import java.io.*;
import java.util.*;
public class TempuratureDifference {
	public static void main (String[]args) throws FileNotFoundException{
	Scanner weather = new Scanner(new File ("./resources/weather.txt"));
	double temp = 0.0;
	double [] tempuratures = new double[8];
	for(int i = 0; i<8; i++){
		tempuratures[i] = weather.nextDouble();
	}
	System.out.println(Arrays.toString(tempuratures));
	double tempChange = weather.nextDouble()-temp;
	System.out.println(tempChange);
	weather.close();
	}
}
